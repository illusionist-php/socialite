    "keywords": ["oauth", "illusionist", "laravel", "socialite"],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
      {
          "name": "kayson",
          "email": "kayson@huida.cn"
      }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "league/oauth1-client": "^1.0"
    },
    "require-dev": {
